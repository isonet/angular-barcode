{
  "name": "angular-barcode",
  "version": "1.2.5",
  "description": "An angular directive for lindell's JsBarcode",
  "main": "dist/barcode.es5.js",
  "scripts": {
    "build-es6": "browserify src/barcode.js --standalone barcode -o dist/barcode.es6.js",
    "build-es5": "browserify src/barcode.js -o dist/barcode.es5.js -t [ babelify --presets [ es2015 ] ]",
    "build": "npm run build-es5 && npm run build-es6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isonet/barcode.git"
  },
  "keywords": [
    "barcode",
    "angular",
    "module",
    "directive",
    "generator",
    "lindell",
    "JsBarcode"
  ],
  "author": "Paul Biester",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isonet/barcode/issues"
  },
  "homepage": "https://github.com/isonet/barcode#readme",
  "dependencies": {},
  "devDependencies": {
    "angular": "^1.5.7",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.1",
    "jsbarcode": "^3.3.17"
  }
}
